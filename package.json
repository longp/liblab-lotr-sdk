{
  "name": "longphan-liblab-lotr-sdk",
  "version": "1.0.0",
  "description": "take home test sdk project for liblab by longphan",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
